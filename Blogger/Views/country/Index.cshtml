
@{
    ViewBag.Title = Model.name;
    Layout = "~/Views/Shared/_Mylayout.cshtml";
}
<!doctype html>
<html class="no-js" lang="zxx">
<body>
    <main>
        <!--? Hero Start -->
        @*<div class="slider-area2">
            <div class="slider-height2 hero-overly d-flex align-items-center">
                <div class="container">
